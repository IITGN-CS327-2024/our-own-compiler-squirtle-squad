digraph {
	Program
	Start -> Program
	Statements
	Program -> Statements
	Statement
	Statements -> Statement
	VariableDeclarationStatement
	Statement -> VariableDeclarationStatement
	var
	VariableDeclarationStatement -> var
	int
	VariableDeclarationStatement -> int
	a
	VariableDeclarationStatement -> a
	"="
	VariableDeclarationStatement -> "="
	FunctionCall
	VariableDeclarationStatement -> FunctionCall
	something
	FunctionCall -> something
	"("
	FunctionCall -> "("
	None
	FunctionCall -> None
	")"
	FunctionCall -> ")"
	";"
	VariableDeclarationStatement -> ";"
	Statement
	Statements -> Statement
	VariableDeclarationStatement
	Statement -> VariableDeclarationStatement
	var
	VariableDeclarationStatement -> var
	int
	VariableDeclarationStatement -> int
	b
	VariableDeclarationStatement -> b
	"="
	VariableDeclarationStatement -> "="
	3
	VariableDeclarationStatement -> 3
	";"
	VariableDeclarationStatement -> ";"
	Statement
	Statements -> Statement
	VariableDeclarationStatement
	Statement -> VariableDeclarationStatement
	var
	VariableDeclarationStatement -> var
	int
	VariableDeclarationStatement -> int
	c
	VariableDeclarationStatement -> c
	"="
	VariableDeclarationStatement -> "="
	AddExpr
	VariableDeclarationStatement -> AddExpr
	a
	AddExpr -> a
	"+"
	AddExpr -> "+"
	PowerExpr
	AddExpr -> PowerExpr
	b
	PowerExpr -> b
	"**"
	PowerExpr -> "**"
	3
	PowerExpr -> 3
	";"
	VariableDeclarationStatement -> ";"
	""
	Start -> ""
}
